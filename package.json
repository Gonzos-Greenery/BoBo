{
  "name": "bobo",
  "version": "1.0.0",
  "description": "BoBo movie recommender",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "nodemon app.js",
    "start-dev": "webpack -w & nodemon server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gonzos-Greenery/BoBo.git"
  },
  "author": "BoBo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gonzos-Greenery/BoBo/issues"
  },
  "homepage": "https://github.com/Gonzos-Greenery/BoBo#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.6",
    "nodemon": "^2.0.18",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "mongodb": "^4.7.0",
    "mongoose": "^6.4.1",
    "react": "^18.2.0"
  }
}
